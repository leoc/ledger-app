<% @report = query do %>
    select
        updated_at as "Updated At",
        num_seconds as "Num Seconds",
        checksum as "Checksum"
    from
        update_history
    order by
        updated_at desc
<% end %>

<section class="content-header">
    <h1>Update History</h1>
</section>
<section class="content">
    <div class="box">
        <div class="box-body">
	    <%= table(@report) %>
	</div>
    </div>
</section>
